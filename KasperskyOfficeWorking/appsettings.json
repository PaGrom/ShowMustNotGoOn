{
    "Database": {
        "ConnectionString": "Data Source=KasperskyOfficeWorking.db"
    },
    "Telegram": {
        "TelegramApiToken": ""
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": { "path": "C:/Logs/KasperskyOfficeWorking/log.txt" }
            }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    }
}